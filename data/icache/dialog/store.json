{
  "type": "minecraft:confirmation",
  "title": {"text": "Store Item", "color": "gold"},
  "body": [
    {
      "type": "plain_message",
      "contents": {"text": "Enter the item ID to cache the held item.", "color": "gray"}
    }
  ],
  "inputs": [
    {
      "type": "minecraft:text",
      "key": "id",
      "label": "Item ID",
      "max_length": 64,
      "width": 200
    }
  ],
  "yes": {
    "label": {"text": "Store"},
    "action": {
      "type": "dynamic/run_command",
      "template": "function icache:cache/store {id:\"$(id)\"}"
    }
  },
  "no": {
    "label": {"text": "Cancel"}
  }
}