{
  "type": "minecraft:confirmation",
  "title": {"text": "Move Item", "color": "gold"},
  "body": [
    {
      "type": "plain_message",
      "contents": {"text": "Enter the item ID and destination folder name.", "color": "gray"}
    }
  ],
  "inputs": [
    {
      "type": "minecraft:text",
      "key": "id",
      "label": "Item ID",
      "max_length": 64,
      "width": 200
    },
    {
      "type": "minecraft:text",
      "key": "folder_name",
      "label": "Folder Name",
      "max_length": 32,
      "width": 200
    }
  ],
  "yes": {
    "label": {"text": "Move"},
    "action": {
      "type": "dynamic/run_command",
      "template": "function icache:admin/folder/open/move_item {id:\"$(id)\",folder_name:\"$(folder_name)\"}"
    }
  },
  "no": {
    "label": {"text": "Cancel"}
  }
}